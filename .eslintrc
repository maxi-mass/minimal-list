{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": ["error", "never"],
    "object-curly-spacing": [1, "always"],
    "no-return-await": "off",
    "no-console": "off",
    "arrow-parens": [2, "as-needed"],
    "comma-dangle": "off",
    "import/no-unresolved": "off"
  },
  "extends": "airbnb"
}
